FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform (0 0 5.6);

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    inlet_fluid
    {
        type            fixedValue;
        value           uniform (0 0 5.6);
    }
    
    outlet_fluid
    {
        type            pressureInletOutletVelocity;
        value           $internalField;
    }
    
    fluid_to_fins
    {
        type            noSlip;
    }

    walls_fluid
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}

// ************************************************************************* //
